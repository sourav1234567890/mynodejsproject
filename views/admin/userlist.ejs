<% include ../partials/admin/header.ejs %>
<% include ../partials/admin/sidebar.ejs %>
 <div class="page-content-wrapper">
    <div class="page-content">
    <div class="row">
                        <div class="col-md-12">
                            <!-- BEGIN EXAMPLE TABLE PORTLET-->
                            <div class="portlet light bordered">
                                <div class="portlet-title">
                                    <div class="caption font-dark">
                                        <i class="icon-settings font-dark"></i>
                                        <span class="caption-subject bold uppercase">User List</span>
                                    </div>
                                    
                                </div>
                                <div class="portlet-body">
                                    <div class="table-toolbar">
                                        <div class="row">
                                            <div class="col-md-6">
                                                <div class="btn-group">
                                                    <button id="sample_editable_1_new" class="btn sbold green"> Add New
                                                        <i class="fa fa-plus"></i>
                                                    </button>
                                                </div>
                                            </div>
                                           
                                        </div>
                                    </div>
                                    <% if(userlist.length>0) {%>
                                    <table class="table table-striped table-bordered table-hover table-checkable order-column" id="sample_1">
                                        <thead>
                                            <tr>
                                                <th>
                                                    Index </th>
                                                <th> Username </th>
                                                <th> Email </th>
                                                
                                                <th> Joined </th>
                                                <th> Status </th>
                                                <th> Action </th>
                                            </tr>
                                        </thead>
                                        <tbody>
                                        <% var i=1; %>
                                        <% userlist.forEach((users,index)=> {  %>
                                            <tr class="odd gradeX">
                                                <td><%= i %></td>
                                                <td> <%=users.first_name %> </td>
                                                <td>
                                                    <a href="mailto:shuxer@gmail.com"> <%=users.email %></a>
                                                </td>
                                                
                                                <td class="center"> 12 Jan 2012 </td>
                                                <td>
                                                <% if(users.status==0){ %>
                                                    <span class="label label-sm label-danger"> approve </span>
                                                    <% } else{ %>
                                                    <span class="label label-sm label-success"> Approved </span>
                                                    <% } %>
                                                </td>
                                                <td>
                                                <% if(users.status==0){ %>
                                                <span data="newval-<%= users.id %>" class="label label-sm label-success new_approve " id="<%= users.id %>"> Approve </span>
                                                <% } else{ %>
                                                <span class="label label-sm label-danger new_block" id="<%= users.id %>"> Block </span>
                                                <% } %>
                                                <img id="newload" src="/assets/img/loader.gif" class="rounded-circle player-img newloader-<%=users.id %>" alt="" " style="display:none;">
                                                </td>
                                            </tr>
                                        <% i++ }) %>
                                            
                                            
                                           

                                        </tbody>
                                    </table>
                                    <% } else{ %>

                                    <p>no user found
                                    <% }  %>
                                </div>
                            </div>
                            <!-- END EXAMPLE TABLE PORTLET-->
                        </div>
                    </div>
    </div>
 </div>

<% include ../partials/admin/footer.ejs %>